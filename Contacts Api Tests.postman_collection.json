		{
			"name": "Get all contacts",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {",
							"    pm.expect(pm.response.code).to.equal(200);",
							"});",
							"pm.test(\"Response is a list\", function () {",
							"    pm.expect(pm.response.json()).to.be.an('array');",
							"});",
							"pm.test(\"Response have all properties\", function () {",
							"     var responseJSON = pm.response.json();",
							"",
							"     responseJSON.forEach((contact) => {",
							"            pm.expect(contact).to.have.property('id');",
							"            pm.expect(contact).to.have.property('age');",
							"            pm.expect(contact).to.have.property('firstName');",
							"            pm.expect(contact).to.have.property('lastName');",
							"            pm.expect(contact).to.have.property('email');",
							"            pm.expect(contact).to.have.property('dateOfBirth');",
							"            pm.expect(contact).to.have.property('phone');",
							"     });",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
